*	ESTE PROGRAMA SIRVE PARA BORRAR LA MEMORIA DEL WOOKIE Y TODOS LOS REGISTROS
*	
*	ASI NO LOS TENES QUE BORRAR A MANO PAPA ;)

	p68h11

	org	$2000
main	equ	*
	
	ldx	#$201D

	ldy	#$2FFF		repeticiones del contador (va en el main, loop o lo que sea)
repite	clr	0,X
	inx
	dey
	bne	repite

	ldd	#0
	ldx	#0
	ldy	#0


fin	jmp	fin
	

	END